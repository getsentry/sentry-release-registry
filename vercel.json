{
  "version": 2,
  "builds": [
    {
      "src": "static-site/cmd/build.go",
      "use": "@vercel/go",
      "config": {
        "buildCommand": "go mod vendor && go build -o output static-site/cmd/build.go"
      }
    }
  ],
  "outputDirectory": "static-site/dist",
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}
